<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="cq:Page">
	<jcr:content
		jcr:primaryType="nt:unstructured"
		jcr:title="GMF MyAccount Design"
		sling:resourceType="wcm/core/components/designer">
		<contentPage jcr:primaryType="nt:unstructured">
			<mainPar 
				jcr:primaryType="nt:unstructured"
				sling:resourceType="wcm/foundation/components/responsivegrid"
				components="[group:GMF MyAccount]">
				<colctrl
					jcr:primaryType="nt:unstructured"
					sling:resourceType="gmf-myaccount/components/content/colctrl"
					layouts="2;cq-colctrl-lt0&#x9;2 Columns (50%, 50%)"/>
				<cq:authoring
					jcr:primaryType="nt:unstructured">
					<assetToComponentMapping jcr:primaryType="nt:unstructured">
						<image
							jcr:primaryType="nt:unstructured"
							assetGroup="media"
							assetMimetype="image/*"
							droptarget="image"
							resourceType="gmf-myaccount/components/content/image"
							type="Images"/>
					</assetToComponentMapping>
				</cq:authoring>
				<section jcr:primaryType="nt:unstructured"/>
			</mainPar>
			<logo
				jcr:primaryType="nt:unstructured"
				sling:resourceType="gmf-myaccount/components/structure/logo"
				absParent="2">
				<image/>
			</logo>
		</contentPage>
	</jcr:content>
</jcr:root>